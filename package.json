{
  "name": "get-taste",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "node ./dist/server/entry.mjs",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "dev:e2e": "cross-env DISABLE_AUTH=true astro dev",
    "test:e2e:clean": "cross-env NODE_ENV=test dotenv -c -e .env.test playwright test",
    "test:e2e": "dotenv -c -e .env.test playwright test",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.tsx,.astro src",
    "format": "prettier --write .",
    "format:check": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,astro,json,md}\"",
    "prepare": "husky install",
    "format:staged": "prettier --write",
    "lint:staged": "eslint --max-warnings=0",
    "tsc": "astro check --watch",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:unit:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "migrate": "ts-node scripts/run-migrations.ts",
    "migrate:content-columns": "node scripts/run-content-columns-migration.js"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "4.2.2",
    "@astrojs/sitemap": "3.3.0",
    "@radix-ui/react-avatar": "^1.1.7",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/vite": "4.0.17",
    "@tanstack/react-query": "^5.74.4",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "astro": "5.5.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.9.2",
    "lucide-react": "^0.487.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.15.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.56.2",
    "server-destroy": "^1.0.1",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "4.0.17",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@astrojs/node": "^9.2.1",
    "@eslint/compat": "1.2.7",
    "@eslint/js": "9.23.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/dotenv": "^8.2.3",
    "@types/node": "^22.15.17",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "@vitest/coverage-v8": "^3.1.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "4.2.5",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-aeaed83-20250323",
    "eslint-plugin-react-hooks": "5.2.0",
    "husky": "^8.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "15.5.0",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "supabase": "^2.22.4",
    "tw-animate-css": "^1.2.8",
    "typescript-eslint": "8.28.0",
    "vitest": "^3.1.2"
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
